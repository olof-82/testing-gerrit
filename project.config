[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/olof-82
[access "refs/for/refs/*"]
	read = group user/olof-82
	push = group user/olof-82
	label-Code-Review = -2..+2 group user/olof-82
	label-Verified = -1..+1 group user/olof-82
	rebase = group user/olof-82
	removeReviewer = group user/olof-82
	submit = group user/olof-82
[access "refs/heads/*"]
	read = group user/olof-82
	create = group user/olof-82
	push = +force group user/olof-82
	pushMerge = group user/olof-82
[access "refs/tags/*"]
	push = +force group user/olof-82
	pushTag = group user/olof-82
	pushSignedTag = group user/olof-82
[receive]
	requireChangeId = true
[submit]
	action = rebase if necessary
